<section class="login-container">
  <h1>Admin Login</h1>
  <%= form_with url: '/login', method: 'post', class: 'form' do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Log In'%>
  <% end %>
<section>
